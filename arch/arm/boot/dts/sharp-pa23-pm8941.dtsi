/*****************************************/
/*  qpnp-vadc                            */
/*****************************************/

&qpnp_vadc {

	chan@0 { /* usb_in */
	};

	chan@1 { /* dc_in */
	};

	chan@2 { /* vchg_sns */
	};

	chan@5 { /* vcoin */
	};

	chan@6 { /* vbat_sns */
//      qcom,decimation = <0>;
//      qcom,pre-div-channel-scaling = <1>;
        qcom,calibration-type = "absolute";
        qcom,scale-function = <0>;
//      qcom,hw-settle-time = <0>;
//      qcom,fast-avg-setup = <0>;
        sharp,scale-function = <6>;
	};

	chan@7 { /* vph_pwr */
	};

	chan@8 { /* die_temp */
	};

	chan@9 { /* ref_625mv */
	};

	chan@a { /* ref_1250v */
	};

	chan@b { /* chg_temp */
        label = "chg_temp";
        reg = <0xb>;
        qcom,decimation = <0>;
        qcom,pre-div-channel-scaling = <0>;
        qcom,calibration-type = "absolute";
        qcom,scale-function = <3>;
        qcom,hw-settle-time = <0>;
        qcom,fast-avg-setup = <0>;
	};

    chan@e { /* gnd_ref */
        label = "gnd_ref";
        reg = <0xe>;
        qcom,decimation = <0>;
        qcom,pre-div-channel-scaling = <0>;
        qcom,calibration-type = "absolute";
        qcom,scale-function = <0>;
        qcom,hw-settle-time = <0>;
        qcom,fast-avg-setup = <0>;
    };

	chan@f { /* vdd_vadc */
        label = "vdd_vadc";
        reg = <0xf>;
        qcom,decimation = <0>;
        qcom,pre-div-channel-scaling = <0>;
        qcom,calibration-type = "absolute";
        qcom,scale-function = <0>;
        qcom,hw-settle-time = <0>;
        qcom,fast-avg-setup = <0>;
	};

	chan@11 { /* mux2_1_1(mpp2) */
        label = "mux2_1_1";
        reg = <0x11>;
        qcom,decimation = <0>;
        qcom,pre-div-channel-scaling = <0>;
        qcom,calibration-type = "absolute";
        qcom,scale-function = <0>;
        qcom,hw-settle-time = <2>;
        qcom,fast-avg-setup = <0>;
	};

	chan@14 { /* mux5_1_1(mpp5) */
        label = "mux5_1_1";
        reg = <0x14>;
        qcom,decimation = <0>;
        qcom,pre-div-channel-scaling = <0>;
        qcom,calibration-type = "absolute";
        qcom,scale-function = <0>;
        qcom,hw-settle-time = <2>;
        qcom,fast-avg-setup = <0>;
	};

	chan@21 { /* chg_cur_out */
        label = "chg_cur_out";
        reg = <0x21>;
        qcom,decimation = <0>;
        qcom,pre-div-channel-scaling = <1>;
        qcom,calibration-type = "absolute";
        qcom,scale-function = <0>;
        qcom,hw-settle-time = <2>;
        qcom,fast-avg-setup = <0>;
	};

	chan@30 { /* batt_therm */
//      qcom,decimation = <0>;
//      qcom,pre-div-channel-scaling = <0>;
        qcom,calibration-type = "ratiometric";
        qcom,scale-function = <1>;
