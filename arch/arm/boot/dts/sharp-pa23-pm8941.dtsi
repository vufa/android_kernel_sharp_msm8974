/*****************************************/
/*  pm8941: qpnp-adc-common              */
/*****************************************/

&spmi_bus {

    qcom,pm8941@0 {

        qpnp_vadc: vadc@3100 {
        };

        qpnp_iadc: iadc@3600 {
        };

        qpnp_vadc_tm: qcom,vadc@3400 {
        };
    };

    qcom,pm8941@1 {

        qpnp_vibrator: qcom,vibrator@c000 {
        };

        qcom,leds@d800 {
            status = "disabled";
        };

        qcom,leds@d900 {
            status = "disabled";
        };

        qcom,leds@da00 {
            status = "disabled";
        };

        qcom,leds@db00 {
            status = "disabled";
        };

        qcom,leds@dc00 {
            status = "disabled";
        };

        qcom,leds@dd00 {
            status = "disabled";
        };

        qcom,leds@de00 {
            status = "disabled";
        };

        qcom,leds@df00 {
            status = "disabled";
        };

        qcom,leds@e000 {
            status = "disabled";
        };

        qcom,leds@e100 {
            status = "disabled";
        };

    };
};

