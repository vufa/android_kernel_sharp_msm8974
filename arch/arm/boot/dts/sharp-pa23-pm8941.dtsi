/*  qpnp-bms                             */
/*****************************************/

&pm8941_bms {
    status = "ok";
    qcom,use-external-rsense;

    qcom,bms-iadc@3800 {
        status = "ok";
    };

    qcom,bms-bms@4000 {
        status = "ok";
    };
};

